<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"]</title>
    <link href="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/lib/twitter-bootstrap/css/bootstrap-grid.css" rel="stylesheet">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css"/>
    <link rel="stylesheet" href="~/lib/admin-lte/css/adminlte.min.css"/>
    <link rel="stylesheet" href="~/lib/datatables/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">  


</head>
<body class="sidebar-mini">
<header>
    <nav class="main-header navbar navbar-expand navbar-white navbar-light">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" data-widget="pushmenu" href="#" role="button">
                    <i class="fas fa-bars"></i>
                </a>
            </li>
        </ul>

        <ul class="navbar-nav ml-auto">
            <li class="nav-item">
                <a asp-action="Logout" asp-controller="Authentification" class="nav-link">
                    <i class="fas  fa-sign-out"></i>
                </a>
            </li>
        </ul>
    </nav>
</header>

<aside class="main-sidebar  sidebar-dark-primary elevation-4">
    <a asp-action="Index" asp-controller="Home" class="brand-link">
        <span class="brand-text font-weight-light ml-4">AML</span>
    </a>
    <div class="sidebar">
        <nav class="mt-2">
            <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                <li class="nav-item">
                    <div href="pages/widgets.html" class="nav-link text-light">
                        <i class="nav-icon fas fa-user"></i>
                        <p class="text-light" id="username">Error User</p>
                    </div>
                </li>
                <li class="nav-item">
                    <a id="newRuleBtn" asp-action="AddNewData" asp-controller="Home" class="nav-link ">
                        <i class="nav-icon fas fa-plus"></i>
                        <p>
                            Yeni Qayda
                        </p>
                    </a>

                </li>
                <li class="nav-item">
                    <a id="rulesBtn" asp-action="Index" asp-controller="Home" class="nav-link">
                        <i class="nav-icon fas fa-th"></i>
                        <p>
                            Qaydalar
                        </p>
                    </a>
                </li>
                <li class="nav-item">
                    <a id="trnTypesBtn" asp-action="TrnTypes" asp-controller="Home" class="nav-link">
                        <i class="nav-icon fas fa-th"></i>
                        <p>
                            Ödəniş növləri
                        </p>
                    </a>
                </li>*
            </ul>
        </nav>
    </div>
</aside>


<div class="content-wrapper">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

 <footer class="footer text-muted" style="height:0"> 
 <div class="container"> 
@*     &copy; 2022 - Constructor - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> 
*@ </div> 
 </footer> 
<script src="~/lib/jquery/jquery.min.js"></script>
<script src="~/lib/admin-lte/js/adminlte.min.js"></script>
<script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/font-awesome/js/all.min.js"></script>
<script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" src="~/lib/inputmask/jquery.inputmask.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>


<script>
</script>

</body>
</html>